@{
    ViewBag.Title = "Joel's Collection";
}

@model IEnumerable<JoelHiltonMovieApp_AdamNeumiller.Models.AddMovieResponse>



<!--This section of the code is the view for the movie collection. -->
<div class="text-center" >

    <h2>Joel's Collection</h2>
    <table class="center" >
        <thead class="center">
            <tr>
            <th>Movie Title</th>
            <th>Category</th>
            <th>Year</th>
            <th>Director</th>
            <th>Rating</th>
            <th>Edited</th>
            <th>Lent To</th>
            <th>Notes</th>
            </tr>
        </thead>
        <tbody>
            
            @foreach (JoelHiltonMovieApp_AdamNeumiller.Models.AddMovieResponse x in Model)
            {
            <tr>
                <td>@x.Title</td>
                <td>@x.Category</td>
                <td>@x.Year</td>
                <td>@x.Director</td>
                <td>@x.Rating</td>
                <td>@x.Edited</td>
                <td>@x.LentTo</td>
                <td>@x.Notes</td>



            </tr>
            }

 
        </tbody>
    </table>


</div>
